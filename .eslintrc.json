{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 2016,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/prop-types": "off",
    "jsx-a11y/anchor-is-valid":["error", {
      "components": ["Link"],
      "specialLink": ["to"]
    }],
    "no-useless-constructor": "off",
    "react/prefer-stateless-function": "off"
  }
}